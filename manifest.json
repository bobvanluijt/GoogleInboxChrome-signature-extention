{
  "manifest_version": 2,

  "name": "GoogleInboxChrome-signature-extention",
  "description": "Creating HTML5 signature emails in Google inbox.",
  "version": "0.1",

  
  "permissions": [
    "https://inbox.google.com/",
	"http://inbox.google.com/",
	"storage",
	"tabs"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
	"default_title": "Adding signature for Google"
  }
  
}